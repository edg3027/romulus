{
  "name": "romulus-svelte",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "build": "pnpm -r build",
