{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^build", "^lint"]
    },
    "lint:fix": {
      "dependsOn": ["^build", "^lint:fix"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    }
  }
}
